<AnchorPane prefHeight="556.0" prefWidth="400.0" style="-fx-background-color: #edf0ee;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
    <children>
        <ImageView fitHeight="123.0" fitWidth="119.0" layoutX="147.0" layoutY="34.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assets/icons8-workstation-80.png" />
            </image>
        </ImageView>
        <Label layoutX="143.0" layoutY="178.0" text="Welcome back !">
            <font>
                <Font name="Ubuntu" size="18.0" />
            </font>
        </Label>
        <Label layoutX="66.0" layoutY="225.0" text="Please enter your login credentials to log in" />
        <Button fx:id="btnSignIn" defaultButton="true" layoutX="128.0" layoutY="388.0" mnemonicParsing="false" onAction="#btnSignIn_OnAction" prefHeight="43.0" prefWidth="157.0" text="Sign In" />
        <ImageView fitHeight="147.0" fitWidth="424.0" layoutX="-4.0" layoutY="431.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assets/ok1re2.png" />
            </image>
        </ImageView>
        <JFXTextField fx:id="txtUserName" layoutX="74.0" layoutY="262.0" prefHeight="36.0" prefWidth="247.0" promptText="Username" />
        <JFXPasswordField fx:id="txtPassword" layoutX="74.0" layoutY="317.0" prefHeight="36.0" prefWidth="247.0" promptText="Password" />
        <ImageView fx:id="closeImg" fitHeight="26.0" fitWidth="24.0" layoutX="364.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assets/icons8-close-50.png" />
            </image>
        </ImageView>
        <JFXButton fx:id="btnClose" buttonType="RAISED" layoutX="361.0" layoutY="20.0" onAction="#btnClose_OnAction" prefHeight="26.0" prefWidth="31.0" text="." />
    </children>
</AnchorPane>
